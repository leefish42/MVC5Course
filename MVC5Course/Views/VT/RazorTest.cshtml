@model int[]

@{
    for (int i = 0; i < Model.Length; i++)
    {
        @Model[i]
    }
}

@{
    for (int i = 0; i < Model.Length; i++)
    {
        var space = " ";
        @Model[i]@space
        @*@: 會把整行變文字*@
    }
}
<ul>
@for (int i = 0; i < Model.Length; i++)
{
  <li>@Model[i]</li>
}
</ul>

